@model NikonRepo.Omos

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<h2>Ændre indhold på om os</h2>
<form action="/Admin/Omos/Index/" method="POST" enctype="multipart/form-data">

    <table class="katTable">
        <tr><td>Overskrift :</td><td><input id="Text1" type="text" name="Overskrift" value="@Model.Overskrift" placeholder="Overskrift" /></td></tr>
        <tr><td>Tekst :</td><td><textarea id="TextArea1" rows="6" cols="20" name="Tekst" placeholder="Tekst">@Model.Tekst</textarea></td></tr>
        <tr><td>Billede :</td><td><input id="File1" type="file" name="Billede" /></td></tr>
        <tr><td colspan="2"><input id="AddkatKnap" type="submit" value="submit" /></td></tr>
        <tr><td></td><td></td></tr>

    </table>
    <input id="Text1" type="hidden" value="@Model.ID"/>


    <input id="Hidden1" type="hidden" name="oldpic" value="@Model.Billede"/>



</form>